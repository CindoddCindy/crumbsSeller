<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.crumbsproject.crumbsseller">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.CrumbsSeller">
        <activity android:name=".view.SellerGetOrder"></activity>
        <activity android:name=".view.SellerAddItem" />
        <activity android:name=".view.BottomNavigationSeller" />
        <activity android:name=".view.LoginSeller" />
        <activity android:name=".view.RegisSeller" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>